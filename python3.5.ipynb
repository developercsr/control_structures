    "#for loop to find the sum of the first n even numbers\n",
    "def main():\n",
    "    sum=0\n",
    "    i=int(input(\"Enter the integers up to you want to add : \"))\n",
    "    for j in range(0,i+1):\n",
    "        if j%2==0:\n",
    "            sum=sum+j\n",
    "    print(sum)\n",
    "if __name__==\"__main__\":\n",
    "    main()\n",
    "\n"
   ]
